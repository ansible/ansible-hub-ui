        "@redhat-cloud-services/frontend-components": "^3.9.34",
        "@types/node": "^16.18.16",
        "@types/react": "^17.0.2",
        "@types/react-dom": "^17.0.2",
        "axios": "~1.3.5",
        "classnames": "^2.3.2",
        "csstype": "^3.1.2",
        "detect-browser": "^5.3.0",
        "file-saver": "^2.0.5",
        "js-cookie": "^3.0.1",
        "moment": "^2.29.4",
        "monaco-editor": "^0.34.1",
        "monaco-yaml": "^4.0.4",
